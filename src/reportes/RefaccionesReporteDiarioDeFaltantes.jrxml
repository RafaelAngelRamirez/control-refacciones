	<queryString>
		<![CDATA[select  
refaccion.nombre,
refaccion.stockMinimo,
refaccion.stockMaximo,
refaccion.codigoInterno,
refaccion.codigoProveedor,
refaccion.descripcion,
importancia.importancia,
unidad.unidad,
existenciarefaccion.existencia
from refaccion 
join importancia on importancia.id=refaccion.importancia 
join unidad on unidad.id = refaccion.unidad 
join existenciarefaccion on existenciarefaccion.idRefaccion= refaccion.id 

WHERE existenciarefaccion.existencia <= refaccion.stockMinimo

ORDER BY importancia.id]]>
	</queryString>
